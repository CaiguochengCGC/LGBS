			<ActionStep id="2" classname="com.hanthink.zzcj.action.UpdateStopLineAction" on1Do="next" onOtherDo="3"/>
			<ActionStep id="3" classname="cn.boho.framework.actions.EndTransactionAction" onOtherDo="return" />
		</ActionParam>
	</Action>
	
	<Action id="ZZCJ_UPDATE_WORKTIME_ACTION">
		<Description>修改油漆车间工作时间</Description>
		<ActionContext>
			<Data name="BAN1S" type="string" /><!--班次1开始时间-->
			<Data name="BAN1E" type="string" /><!--班次1结束时间-->
			<Data name="BAN2S" type="string" /><!--班次2开始时间-->
			<Data name="BAN2E" type="string" /><!--班次2结束时间-->
			<Data name="BAN3S" type="string" /><!--班次3开始时间-->
			<Data name="BAN3E" type="string" /><!--班次3结束时间-->
			<Data name="BAN1SMIN" type="string" /><!--班次1开始时间-->
			<Data name="BAN1EMIN" type="string" /><!--班次1结束时间-->
			<Data name="BAN2SMIN" type="string" /><!--班次2开始时间-->
			<Data name="BAN2EMIN" type="string" /><!--班次2结束时间-->
			<Data name="BAN3SMIN" type="string" /><!--班次3开始时间-->
			<Data name="BAN3EMIN" type="string" /><!--班次3结束时间-->
		</ActionContext>
		<ActionParam>
			<ActionStep id="1" classname="cn.boho.framework.actions.BeginTransactionAction" on1Do="next" onOtherDo="3" />
			<ActionStep id="2" classname="com.hanthink.zzcj.action.UpdateWorkTimeAction" on1Do="next" onOtherDo="3"/>
			<ActionStep id="3" classname="cn.boho.framework.actions.EndTransactionAction" onOtherDo="return" />
		</ActionParam>
	</Action>
	
	
	
	<Action id="ZZCJ_DELETE_STOPLINE_ACTION">
